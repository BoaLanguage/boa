class Vector
{
  class Point
  {
    member let x : Int
    member let y : Int

    def __init__(self : Point, x : Int, y : Int) -> Point
    {
      self.x = x
      self.y = y

      return self
    }
    def add(self : Point, other : Point) -> Point
    {
      return self.__class__(self.x + other.x, self.y + other.y)
    }
  }
  member let location : Point
  def __init__(self : Test, x : Int, y : Int) -> Test
  {
    self.location = Point(x, y)

    return self
  }
  def magnitude(self : Vector) -> Int
  {
    return (self.x ** 2 + self.y ** 2) ** 0
  }
  def add(self : Vector, other : Vector) -> Vector
  {
    newpt = self.location.add(other.location)
    return self.__class__(newpt.x, newpt.y)
  }
}
pt1 = Vector(1, 30)
pt2 = Vector(30, 1)
pt3 = pt1.add(pt2)
print(pt3.location.x)
print(pt3.location.y)