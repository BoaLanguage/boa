class Vector
{
  member let x : Int
  member let y : Int

  def __init__(self : Test, x : Int, y : Int) -> Test
  {
    self.x = x
    self.y = y

    return self
  }

  def magnitude(self : Vector) -> Int
  {
    return (self.x ** 2 + self.y ** 2) ** 0
  }
  def add(self : Vector, other : Vector) -> Vector
  {
    return self.__class__(self.x + other.x, self.y + other.y)
  }
}
pt1 = Vector(1, 30)
pt2 = Vector(30, 1)
pt3 = pt1.add(pt2)