class Vector:

  member var x : Int
  member var y : Int

  def __init__(self : Vector, x : Int, y : Int) -> Vector:
    self.x = x
    self.y = y

    return self

  def add(self : Vector, other : Vector) -> Vector:
    self.x = self.x + other.x
    self.y = self.y + other.y

    return self

  def mult(self : Vector, c : Int) -> Vector:
    self.x = self.x * c
    self.y = self.y * c

    return self

x = Vector(1, 4)
adder = Vector(1, 1)

while True:
  print("_ . _")
  x.add(adder)